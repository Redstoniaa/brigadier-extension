{
  "schemaVersion": 1,
  "id": "brigadier-extension-api",
  "version": "${version}",

  "name": "Brigadier Extension API",
  "description": "Extension to the Brigadier command library, making it easier to create commands, but harder for everyone else who has to download yet another dependency mod.",
  "authors": [
  ],
  "contact": {
    "sources": "https://github.com/Redstoniaa/brigadier-extension"
  },

  "license": "MIT",
  "icon": "assets/brigadierextension/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "brigadierextension.BrigadierExtensionAPI"
    ]
  },
  "mixins": [
    "brigadierextension.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
  }
}
